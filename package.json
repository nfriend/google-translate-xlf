    "chalk": "^2.4.1",
    "google-translate-api": "^2.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "xml2js": "^0.4.19",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "proxyquire": "^2.0.1"
