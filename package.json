{
  "name": "xlf2xlf",
  "version": "1.0.18",
  "description": "A command-line utility to automatically translate .xlf translation files using Google Translate",
  "main": "./src/index.js",
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha",
    "test:watch": "node ./node_modules/mocha/bin/mocha --watch"
