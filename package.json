    "xlf",
    "angular",
    "i18n",
    "multilingual",
    "google-translate",
    "angular2",
    "ng"
  ],
  "author": "Anton Cherepov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chekit/xlf2xlf/issues"
  },
  "homepage": "https://github.com/chekit/xlf2xlf#readme",
  "preferGlobal": true,
  "bin": {
    "xlf2xlf": "./src/index.js"
  },
  "dependencies": {
    "@vitalets/google-translate-api": "^8.0.0",
    "bottleneck": "2.19.5",
    "chalk": "2.4.1",
    "lodash.clonedeep": "4.5.0",
    "xml-js": "1.6.11",
    "yargs": "15.3.1"
